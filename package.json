{
  "name": "screeps-lru-cache",
  "version": "2.1.5",
  "description": "Screeps LRU Cache",
  "author": "Morten Broesby-Olsen",
  "originalAuthor": "Robert Herber",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "esm/index.js",
  "keywords": [
    "cache",
    "lru",
    "typescript",
    "screeps"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "benchmark": "ts-node ./src/benchmark/index.ts",
    "prebuild": "npm run clean",
    "clean": "run-s clean:*",
    "clean:cjs": "rm -rf ./dist",
    "clean:esm": "rm -rf ./esm",
    "build": "run-s build:*",
    "build:cjs": "tsc --module commonjs --outDir dist",
    "build:esm": "tsc --module es2020 --outDir esm",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prettier": "prettier --write **/*.{ts,js,css,html,json} !**/node_modules/** !**/dist/** !**/coverage/** !**/compiled/**",
    "prerelease": "npm run build",
    "release": "np --no-cleanup --no-2fa",
    "test": "jest --coverage",
    "test-watch": "jest --watchAll --coverage",
    "test-clean": "jest --clearCache",
    "version": "npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/benchmark": "^2.1.0",
    "@types/jest": "^26.0.20",
    "@types/screeps": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "benchmark": "^2.1.4",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint": "^7.18.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "np": "^7.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "^2.2.1",
    "screeps-jest": "eduter/screeps-jest#1.2.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
